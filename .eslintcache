th > 0 && messages.map(message => (\n            <Message \n                key={message.timestamp} \n                message={message}\n                user={this.state.user} />\n        ))\n    }\n\n    render() {\n        const { messagesRef, messages } = this.state;\n\n        return (\n            <div className=\"messages\">\n                <MessagesHeader />\n\n                {this.displayMessages(messages)}\n\n                <MessageForm\n                    messagesRef={messagesRef}\n                />\n            </div>\n        );\n    }\n}\n\nconst mapStateToProps = state => ({\n    currentUser: state.user.currentUser,\n    modalShow: state.modal.modalShow,\n    currentChannel: state.channel.currentChannel\n})\n\nexport default connect(mapStateToProps)(Messages);\n","/Users/oishi/projects/React/channels/src/components/ColorPanel.js",[],"/Users/oishi/projects/React/channels/src/components/MetaPanel.js",[],"/Users/oishi/projects/React/channels/src/components/sidePanel/SidePanel.js",[],"/Users/oishi/projects/React/channels/src/components/sidePanel/UserPanel.js",[],"/Users/oishi/projects/React/channels/src/components/sidePanel/Channels.js",[],"/Users/oishi/projects/React/channels/src/components/Modal.js",[],"/Users/oishi/projects/React/channels/src/components/sidePanel/AddChannel.js",[],"/Users/oishi/projects/React/channels/src/components/ChannelInfo.js",[],"/Users/oishi/projects/React/channels/src/components/MessagesHeader.js",[],"/Users/oishi/projects/React/channels/src/components/MessageForm.js",[],"/Users/oishi/projects/React/channels/src/components/Message.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":15,"column":8,"nodeType":"97","endLine":15,"endColumn":24,"suggestions":"98"},"no-native-reassign",["99"],"no-negated-in-lhs",["100"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'addListeners'. Either include it or remove the dependency arra